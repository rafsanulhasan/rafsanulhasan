{
	"version": "1.0",
	"defaultProvider": "cdnjs",
	"libraries": [
		{
			"library": "bootstrap@4.6.0",
			"provider": "unpkg",
			"files": [
				"dist/js/bootstrap.js",
				"dist/js/bootstrap.js.map",
				"dist/js/bootstrap.min.js",
				"dist/js/bootstrap.min.js.map",
				"dist/css/bootstrap.css",
				"dist/css/bootstrap.css.map",
				"dist/css/bootstrap.min.css",
				"dist/css/bootstrap.min.css.map"
			],
			"destination": "wwwroot/libraries/bootstrap"
		},
		{
			"library": "open-iconic@1.1.1",
			"files": [
				"font/css/open-iconic-bootstrap.css",
				"font/css/open-iconic-bootstrap.min.css",
				"font/css/open-iconic.css",
				"font/css/open-iconic.min.css",
				"font/fonts/open-iconic.eot",
				"font/fonts/open-iconic.otf",
				"font/fonts/open-iconic.svg",
				"font/fonts/open-iconic.ttf",
				"font/fonts/open-iconic.woff",
				"png/account-login-2x.png",
				"png/account-login-3x.png",
				"png/account-login-4x.png",
				"png/account-login-6x.png",
				"png/account-login-8x.png",
				"png/account-login.png",
				"png/account-logout.png"
			],
			"destination": "wwwroot/libraries/open-iconic"
		}		
	]
}