    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (server-app)",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["run"],
            "cwd": "${workspaceFolder}/sources/Applications/Web/SPA/Server/"
        },
        {
            "name": ".NET Core Watch (server-app)",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [ "watch", "run" ],
            "cwd": "${workspaceFolder}/sources/Applications/Web/SPA/Server/"
        }
    ]
}